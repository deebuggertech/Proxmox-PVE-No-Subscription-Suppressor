{
  "manifest_version": 3,
  "name": "Proxmox PVE No-Subscription Suppressor",
  "version": "1.3",
  "description": "Suppresses the 'No valid subscription' popup in Proxmox VE.",
  "icons": {
    "1024": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [],
  "author": "fred_approved",
  "browser_specific_settings": {
    "gecko": {
      "id": "{a1b2c3d4-5e6f-7890-ab12-cdef34567890}",
      "strict_min_version": "91.0"
    }
  }
}
